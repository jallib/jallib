-- ------------------------------------------------
--    Symbolic Fuse Definitions
-- ------------------------------------------------
--
-- CONFIG1 (0x8007)
--
pragma fuse_def OSC:0  0x7 {             -- FEXTOSC External Oscillator mode Selection bits
       ECH_NOCLKOUT = 0x7                -- EC (external clock) above 8 MHz
       ECM_NOCLKOUT = 0x6                -- EC (external clock) for 100 kHz to 8 MHz
       ECL_NOCLKOUT = 0x5                -- EC (external clock) below 100 kHz
       OFF = 0x4                         -- Oscillator not enabled
       HS = 0x2                          -- HS (crystal oscillator) above 4 MHz
       XT = 0x1                          -- XT (crystal oscillator) from 100 kHz to 4 MHz
       LP = 0x0                          -- LP (crystal oscillator) optimized for 32.768 kHz
       }
pragma fuse_def RSTOSC:0  0x70 {         -- Power-up default value for COSC bits
       EXT1X = 0x70                      -- EXTOSC operating per FEXTOSC bits 
       HFINT1 = 0x60                     -- HFINTOSC (1MHz)
       LFINT = 0x40                      -- LFINTOSC (31kHz)
       SOSC = 0x30                       -- SOSC (31kHz)
       EXT4X = 0x10                      -- EXTOSC with 4x PLL, with EXTOSC operating per FEXTOSC bits
       HFINT32 = 0x0                     -- HFINTOSC with 2x PLL (32MHz)
       }
pragma fuse_def CLKOUTEN:0  0x100 {      -- Clock Out Enable bit
       DISABLED = 0x100                  -- CLKOUT function is disabled; I/O or oscillator function on OSC2
       ENABLED = 0x0                     -- CLKOUT function is enabled; FOSC/4 clock appears at OSC2
       }
pragma fuse_def CSWEN:0  0x800 {         -- Clock Switch Enable bit
       ENABLED = 0x800                   -- Writing to NOSC and NDIV is allowed
       DISABLED = 0x0                    -- The NOSC and NDIV bits cannot be changed by user software
       }
pragma fuse_def FCMEN:0  0x2000 {        -- Fail-Safe Clock Monitor Enable
       ENABLED = 0x2000                  -- Fail-Safe Clock Monitor is enabled
       DISABLED = 0x0                    -- Fail-Safe Clock Monitor is disabled
       }
--
-- CONFIG2 (0x8008)
--
pragma fuse_def MCLR:1  0x1 {            -- Master Clear Enable bit
       EXTERNAL = 0x1                    -- MCLR/VPP pin function is MCLR; Weak pull-up enabled
       INTERNAL = 0x0                    -- MCLR/VPP pin function is digital input; MCLR internally disabled; Weak pull-up under control of port pin's WPU control bit.
       }
pragma fuse_def PWRTE:1  0x2 {           -- Power-up Timer Enable bit
       DISABLED = 0x2                    -- PWRT disabled
       ENABLED = 0x0                     -- PWRT enabled
       }
pragma fuse_def WDT:1  0xC {             -- Watchdog Timer Enable bits
       ENABLED = 0xC                     -- WDT enabled, SWDTEN is ignored
       RUNONLY = 0x8                     -- WDT enabled while running and disabled in SLEEP/IDLE; SWDTEN is ignored
       CONTROL = 0x4                     -- WDT controlled by the SWDTEN bit in the WDTCON register
       DISABLED = 0x0                    -- WDT disabled; SWDTEN is ignored
       }
pragma fuse_def LPBOREN:1  0x20 {        -- Low-power BOR enable bit
       DISABLED = 0x20                   -- ULPBOR disabled
       ENABLED = 0x0                     -- ULPBOR enabled
       }
pragma fuse_def BROWNOUT:1  0xC0 {       -- Brown-out Reset Enable bits
       ENABLED = 0xC0                    -- Brown-out Reset enabled, SBOREN bit ignored
       RUNONLY = 0x80                    -- Brown-out Reset enabled while running, disabled in Sleep; SBOREN is ignored
       CONTROL = 0x40                    -- Brown-out Reset enabled according to SBOREN
       DISABLED = 0x0                    -- Brown-out Reset disabled
       }
pragma fuse_def VOLTAGE:1  0x200 {       -- Brown-out Reset Voltage selection bit
       V25 = 0x200                       -- Brown-out voltage (Vbor) set to 2.45V
       V27 = 0x0                         -- Brown-out voltage (Vbor) set to 2.7V
       }
pragma fuse_def PPS1WAY:1  0x800 {       -- PPSLOCK bit One-Way Set Enable bit
       ENABLED = 0x800                   -- The PPSLOCK bit can be cleared and set only once; PPS registers remain locked after one clear/set cycle
       DISABLED = 0x0                    -- The PPSLOCK bit can be set and cleared repeatedly (subject to the unlock sequence)
       }
pragma fuse_def STVR:1  0x1000 {         -- Stack Overflow/Underflow Reset Enable bit
       ENABLED = 0x1000                  -- Stack Overflow or Underflow will cause a Reset
       DISABLED = 0x0                    -- Stack Overflow or Underflow will not cause a Reset
       }
pragma fuse_def DEBUG:1  0x2000 {        -- Debugger enable bit
       DISABLED = 0x2000                 -- Background debugger disabled
       ENABLED = 0x0                     -- Background debugger enabled
       }
--
-- CONFIG3 (0x8009)
--
pragma fuse_def WRT:2  0x3 {             -- User NVM self-write protection bits
       DISABLED = 0x3                    -- Write protection off
       BOOT_BLOCK = 0x2                  -- 0000h to 01FFh write-protected, 0200h to 0FFFh may be modified
       HALF = 0x1                        -- 0000h to 07FFh write-protected, 0800h to 0FFFh may be modified
       ENABLED = 0x0                     -- 0000h to 0FFFh write protected, no addresses may be modified
       }
pragma fuse_def LVP:2  0x2000 {          -- Low Voltage Programming Enable bit
       ENABLED = 0x2000                  -- Low Voltage programming enabled. MCLR/VPP pin function is MCLR. MCLRE configuration bit is ignored.
       DISABLED = 0x0                    -- High Voltage on MCLR/VPP must be used for programming.
       }
--
-- CONFIG4 (0x800A)
--
pragma fuse_def CP:3  0x1 {              -- User NVM Program Memory Code Protection bit
       DISABLED = 0x1                    -- User NVM code protection disabled
       ENABLED = 0x0                     -- User NVM code protection enabled
       }
pragma fuse_def CPD:3  0x2 {             -- Data NVM Memory Code Protection bit
       DISABLED = 0x2                    -- Data NVM code protection disabled
       ENABLED = 0x0                     -- Data NVM code protection enabled
       }
--
