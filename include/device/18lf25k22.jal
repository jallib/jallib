-- Free shared memory: 0x1-0x5F
--
-- ===================================================
--
--    Symbolic Fuse Definitions
-- ------------------------------
--
-- CONFIG1H (0x300001)
--
pragma fuse_def OSC:1 0xF {                -- Oscillator Selection bits
       ECL_NOCLKOUT = 0xD                  -- EC oscillator (low power, <500 kHz)
       ECL_CLKOUT = 0xC                    -- EC oscillator, CLKOUT function on OSC2 (low power, <500 kHz)
       ECM_NOCLKOUT = 0xB                  -- EC oscillator (medium power, 500 kHz-16 MHz)
       ECM_CLKOUT = 0xA                    -- EC oscillator, CLKOUT function on OSC2 (medium power, 500 kHz-16 MHz)
       INTOSC_CLKOUT = 0x9                 -- Internal oscillator block, CLKOUT function on OSC2
       INTOSC_NOCLKOUT = 0x8               -- Internal oscillator block
       RC_NOCLKOUT = 0x7                   -- External RC oscillator
       RC_CLKOUT = 0x6                     -- External RC oscillator, CLKOUT function on OSC2
       ECH_NOCLKOUT = 0x5                  -- EC oscillator (high power, >16 MHz)
       ECH_CLKOUT = 0x4                    -- EC oscillator, CLKOUT function on OSC2 (high power, >16 MHz)
       HSM = 0x3                           -- HS oscillator (medium power 4-16 MHz)
       HSH = 0x2                           -- HS oscillator (high power > 16 MHz)
       XT = 0x1                            -- XT oscillator
       LP = 0x0                            -- LP oscillator
       }
pragma fuse_def PLLEN:1 0x10 {             -- 4X PLL Enable
       P1 = 0x0                            -- Oscillator used directly
       P4 = 0x10                           -- Oscillator multiplied by 4
       }
pragma fuse_def PRICLKEN:1 0x20 {          -- Primary clock enable bit
       DISABLED = 0x0                      -- Primary clock disabled
       ENABLED = 0x20                      -- Primary clock is always enabled
       }
pragma fuse_def FCMEN:1 0x40 {             -- Fail-Safe Clock Monitor Enable bit
       DISABLED = 0x0                      -- Fail-Safe Clock Monitor disabled
       ENABLED = 0x40                      -- Fail-Safe Clock Monitor enabled
       }
pragma fuse_def IESO:1 0x80 {              -- Internal/External Oscillator Switchover bit
       DISABLED = 0x0                      -- Oscillator Switchover mode disabled
       ENABLED = 0x80                      -- Oscillator Switchover mode enabled
       }
--
-- CONFIG2L (0x300002)
--
pragma fuse_def PWRTE:2 0x1 {              -- Power-up Timer Enable bit
       DISABLED = 0x1                      -- Power up timer disabled
       ENABLED = 0x0                       -- Power up timer enabled
       }
pragma fuse_def BROWNOUT:2 0x6 {           -- Brown-out Reset Enable bits
       ENABLED = 0x6                       -- Brown-out Reset enabled in hardware only (SBOREN is disabled)
       RUNONLY = 0x4                       -- Brown-out Reset enabled in hardware only and disabled in Sleep mode (SBOREN is disabled)
       CONTROL = 0x2                       -- Brown-out Reset enabled and controlled by software (SBOREN is enabled)
       DISABLED = 0x0                      -- Brown-out Reset disabled in hardware and software
       }
pragma fuse_def VOLTAGE:2 0x18 {           -- Brown Out Reset Voltage bits
       V19 = 0x18                          -- VBOR set to 1.90 V nominal
       V22 = 0x10                          -- VBOR set to 2.20 V nominal
       V25 = 0x8                           -- VBOR set to 2.50 V nominal
       V28 = 0x0                           -- VBOR set to 2.85 V nominal
       }
--
-- CONFIG2H (0x300003)
--
pragma fuse_def WDT:3 0x3 {                -- Watchdog Timer Enable bits
       ENABLED = 0x3                       -- WDT is always enabled. SWDTEN bit has no effect
       CONTROL = 0x2                       -- WDT is controlled by SWDTEN bit of the WDTCON register
       RUNONLY = 0x1                       -- WDT is disabled in sleep, otherwise enabled. SWDTEN bit has no effect
       DISABLED = 0x0                      -- Watch dog timer is always disabled. SWDTEN has no effect.
       }
pragma fuse_def WDTPS:3 0x3C {             -- Watchdog Timer Postscale Select bits
       P32K = 0x3C                         -- 1:32768
       P16K = 0x38                         -- 1:16384
       P8K = 0x34                          -- 1:8192
       P4K = 0x30                          -- 1:4096
       P2K = 0x2C                          -- 1:2048
       P1K = 0x28                          -- 1:1024
       P512  = 0x24                        -- 1:512
       P256  = 0x20                        -- 1:256
       P128  = 0x1C                        -- 1:128
       P64  = 0x18                         -- 1:64
       P32  = 0x14                         -- 1:32
       P16  = 0x10                         -- 1:16
       P8  = 0xC                           -- 1:8
       P4  = 0x8                           -- 1:4
       P2  = 0x4                           -- 1:2
       P1  = 0x0                           -- 1:1
       }
--
-- CONFIG3H (0x300005)
--
pragma fuse_def CCP2MUX:5 0x1 {            -- CCP2 MUX bit
       pin_C1 = 0x1                        -- CCP2 input/output is multiplexed with RC1
       pin_B3 = 0x0                        -- CCP2 input/output is multiplexed with RB3
       }
pragma fuse_def PBADEN:5 0x2 {             -- PORTB A/D Enable bit
       ANALOG = 0x2                        -- PORTB<5:0> pins are configured as analog input channels on Reset
       DIGITAL = 0x0                       -- PORTB<5:0> pins are configured as digital I/O on Reset
       }
pragma fuse_def CCP3MUX:5 0x4 {            -- P3A/CCP3 Mux bit
       pin_B5 = 0x4                        -- P3A/CCP3 input/output is multiplexed with RB5
       pin_C6 = 0x0                        -- P3A/CCP3 input/output is mulitplexed with RC6
       }
pragma fuse_def HFOFST:5 0x8 {             -- HFINTOSC Fast Start-up
       ENABLED = 0x8                       -- HFINTOSC output and ready status are not delayed by the oscillator stable status
       DISABLED = 0x0                      -- HFINTOSC output and ready status are delayed by the oscillator stable status
       }
pragma fuse_def T3CKMUX:5 0x10 {           -- Timer3 Clock input mux bit
       pin_C0 = 0x10                       -- T3CKI is on RC0
       pin_B5 = 0x0                        -- T3CKI is on RB5
       }
pragma fuse_def P2BMUX:5 0x20 {            -- ECCP2 B output mux bit
       pin_B5 = 0x20                       -- P2B is on RB5
       pin_C0 = 0x0                        -- P2B is on RC0
       }
pragma fuse_def MCLR:5 0x80 {              -- MCLR Pin Enable bit
       EXTERNAL = 0x80                     -- MCLR pin enabled, RE3 input pin disabled
       INTERNAL = 0x0                      -- RE3 input pin enabled; MCLR disabled
       }
--
-- CONFIG4L (0x300006)
--
pragma fuse_def STVR:6 0x1 {               -- Stack Full/Underflow Reset Enable bit
       ENABLED = 0x1                       -- Stack full/underflow will cause Reset
       DISABLED = 0x0                      -- Stack full/underflow will not cause Reset
       }
pragma fuse_def LVP:6 0x4 {                -- Single-Supply ICSP Enable bit
       ENABLED = 0x4                       -- Single-Supply ICSP enabled if MCLRE is also 1
       DISABLED = 0x0                      -- Single-Supply ICSP disabled
       }
pragma fuse_def XINST:6 0x40 {             -- Extended Instruction Set Enable bit
       ENABLED = 0x40                      -- Instruction set extension and Indexed Addressing mode enabled
       DISABLED = 0x0                      -- Instruction set extension and Indexed Addressing mode disabled (Legacy mode)
       }
pragma fuse_def DEBUG:6 0x80 {             -- Background Debug
       DISABLED = 0x80                     -- Disabled
       ENABLED = 0x0                       -- Enabled
       }
--
-- CONFIG5L (0x300008)
--
pragma fuse_def CP0:8 0x1 {                -- Code Protection Block 0
       DISABLED = 0x1                      -- Block 0 (000800-001FFFh) not code-protected
       ENABLED = 0x0                       -- Block 0 (000800-001FFFh) code-protected
       }
pragma fuse_def CP1:8 0x2 {                -- Code Protection Block 1
       DISABLED = 0x2                      -- Block 1 (002000-003FFFh) not code-protected
       ENABLED = 0x0                       -- Block 1 (002000-003FFFh) code-protected
       }
pragma fuse_def CP2:8 0x4 {                -- Code Protection Block 2
       DISABLED = 0x4                      -- Block 2 (004000-005FFFh) not code-protected
       ENABLED = 0x0                       -- Block 2 (004000-005FFFh) code-protected
       }
pragma fuse_def CP3:8 0x8 {                -- Code Protection Block 3
       DISABLED = 0x8                      -- Block 3 (006000-007FFFh) not code-protected
       ENABLED = 0x0                       -- Block 3 (006000-007FFFh) code-protected
       }
--
-- CONFIG5H (0x300009)
--
pragma fuse_def CPB:9 0x40 {               -- Boot Block Code Protection bit
       DISABLED = 0x40                     -- Boot block (000000-0007FFh) not code-protected
       ENABLED = 0x0                       -- Boot block (000000-0007FFh) code-protected
       }
pragma fuse_def CPD:9 0x80 {               -- Data EEPROM Code Protection bit
       DISABLED = 0x80                     -- Data EEPROM not code-protected
       ENABLED = 0x0                       -- Data EEPROM code-protected
       }
--
-- CONFIG6L (0x30000A)
--
pragma fuse_def WRT0:10 0x1 {              -- Write Protection Block 0
       DISABLED = 0x1                      -- Block 0 (000800-001FFFh) not write-protected
       ENABLED = 0x0                       -- Block 0 (000800-001FFFh) write-protected
       }
pragma fuse_def WRT1:10 0x2 {              -- Write Protection Block 1
       DISABLED = 0x2                      -- Block 1 (002000-003FFFh) not write-protected
       ENABLED = 0x0                       -- Block 1 (002000-003FFFh) write-protected
       }
pragma fuse_def WRT2:10 0x4 {              -- Write Protection Block 2
       DISABLED = 0x4                      -- Block 2 (004000-005FFFh) not write-protected
       ENABLED = 0x0                       -- Block 2 (004000-005FFFh) write-protected
       }
pragma fuse_def WRT3:10 0x8 {              -- Write Protection Block 3
       DISABLED = 0x8                      -- Block 3 (006000-007FFFh) not write-protected
       ENABLED = 0x0                       -- Block 3 (006000-007FFFh) write-protected
       }
--
-- CONFIG6H (0x30000B)
--
pragma fuse_def WRTC:11 0x20 {             -- Configuration Register Write Protection bit
       DISABLED = 0x20                     -- Configuration registers (300000-3000FFh) not write-protected
       ENABLED = 0x0                       -- Configuration registers (300000-3000FFh) write-protected
       }
pragma fuse_def WRTB:11 0x40 {             -- Boot Block Write Protection bit
       DISABLED = 0x40                     -- Boot Block (000000-0007FFh) not write-protected
       ENABLED = 0x0                       -- Boot Block (000000-0007FFh) write-protected
       }
pragma fuse_def WRTD:11 0x80 {             -- Data EEPROM Write Protection bit
       DISABLED = 0x80                     -- Data EEPROM not write-protected
       ENABLED = 0x0                       -- Data EEPROM write-protected
       }
--
-- CONFIG7L (0x30000C)
--
pragma fuse_def EBTR0:12 0x1 {             -- Table Read Protection Block 0
       DISABLED = 0x1                      -- Block 0 (000800-001FFFh) not protected from table reads executed in other blocks
       ENABLED = 0x0                       -- Block 0 (000800-001FFFh) protected from table reads executed in other blocks
