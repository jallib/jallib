   var byte _saved_sfr = _control_sfr
   _control_sfr = _control_sfr & (!0xf)
   _control_sfr = _control_sfr | 0x9
   SSPMSK_ = x
   _control_sfr = _saved_sfr
end procedure
function SSPMSK'get() return byte is
   var volatile byte _control_sfr at 0x14
   var byte _saved_sfr = _control_sfr
   var byte x
   _control_sfr = _control_sfr & (!0xf)
   _control_sfr = _control_sfr | 0x9
   x = SSPMSK_
   _control_sfr = _saved_sfr
   return  x
end function
