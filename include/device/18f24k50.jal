const word USB_BDT_ADDRESS  = 0x0400
--
-- Free shared memory: 0x1-0x5F
--
-- ------------------------------------------------
--    Symbolic Fuse Definitions
-- ------------------------------------------------
--
-- CONFIG1L (0x300000)
--
pragma fuse_def PLLSEL:0  0x1 {          -- PLL Selection
       PLL3X = 0x1                       -- 3x clock multiplier
       PLL4X = 0x0                       -- 4x clock multiplier
       }
pragma fuse_def PLLEN:0  0x2 {           -- PLL Enable Configuration bit
       ENABLED = 0x2                     -- PLL Enabled
       DISABLED = 0x0                    -- PLL Disabled (firmware controlled)
       }
pragma fuse_def CPUDIV:0  0x18 {         -- CPU System Clock Postscaler
       P6 = 0x18                         -- CPU uses system clock divided by 6
       P3 = 0x10                         -- CPU uses system clock divided by 3
       P2 = 0x8                          -- CPU uses system clock divided by 2
       P1 = 0x0                          -- CPU uses system clock (no divide)
       }
pragma fuse_def LS48MHZ:0  0x20 {        -- Low Speed USB mode with 48 MHz system clock
       P8 = 0x20                         -- System clock at 48 MHz, USB clock divider is set to 8
       P4 = 0x0                          -- System clock at 24 MHz, USB clock divider is set to 4
       }
--
-- CONFIG1H (0x300001)
--
pragma fuse_def OSC:1  0xF {             -- Oscillator Selection
       ECL_NOCLKOUT = 0xD                -- EC oscillator, low power <4MHz
       ECL_CLKOUT = 0xC                  -- EC oscillator, low power <4MHz, clock output on OSC2
       ECM_NOCLKOUT = 0xB                -- EC oscillator, medium power 4MHz to 16MHz
       ECM_CLKOUT = 0xA                  -- EC oscillator, medium power 4MHz to 16MHz, clock output on OSC2
       ECH_NOCLKOUT = 0x5                -- EC oscillator, high power 16MHz to 48MHz
       ECH_CLKOUT = 0x4                  -- EC oscillator, high power 16MHz to 48MHz, clock output on OSC2
       RC_NOCLKOUT = 0x7                 -- External RC oscillator
       RC_CLKOUT = 0x6                   -- External RC oscillator, clock output on OSC2
       INTOSC_NOCLKOUT = 0x8             -- Internal oscillator
       INTOSC_CLKOUT = 0x9               -- Internal oscillator, clock output on OSC2
       HSM = 0x3                         -- HS oscillator, medium power 4MHz to 16MHz
       HSH = 0x2                         -- HS oscillator, high power 16MHz to 25MHz
       XT = 0x1                          -- XT oscillator
       LP = 0x0                          -- LP oscillator
       }
pragma fuse_def PCLKEN:1  0x20 {         -- Primary Oscillator Shutdown
       ENABLED = 0x20                    -- Primary oscillator enabled
       DISABLED = 0x0                    -- Primary oscillator shutdown firmware controlled
       }
pragma fuse_def FCMEN:1  0x40 {          -- Fail-Safe Clock Monitor
       ENABLED = 0x40                    -- Fail-Safe Clock Monitor enabled
       DISABLED = 0x0                    -- Fail-Safe Clock Monitor disabled
       }
pragma fuse_def IESO:1  0x80 {           -- Internal/External Oscillator Switchover
       ENABLED = 0x80                    -- Oscillator Switchover mode enabled
       DISABLED = 0x0                    -- Oscillator Switchover mode disabled
       }
--
-- CONFIG2L (0x300002)
--
pragma fuse_def PWRTE:2  0x1 {           -- Power-up Timer Enable
       DISABLED = 0x1                    -- Power up timer disabled
       ENABLED = 0x0                     -- Power up timer enabled
       }
pragma fuse_def BROWNOUT:2  0x6 {        -- Brown-out Reset Enable
       ENABLED = 0x6                     -- BOR enabled in hardware (SBOREN is ignored)
       RUNONLY = 0x4                     -- BOR enabled in hardware, disabled in Sleep mode (SBOREN is ignored)
       CONTROL = 0x2                     -- BOR controlled by firmware (SBOREN is enabled)
       DISABLED = 0x0                    -- BOR disabled in hardware (SBOREN is ignored)
       }
pragma fuse_def VOLTAGE:2  0x18 {        -- Brown-out Reset Voltage
       V19 = 0x18                        -- BOR set to 1.9V nominal
       V22 = 0x10                        -- BOR set to 2.2V nominal
       V25 = 0x8                         -- BOR set to 2.5V nominal
       V29 = 0x0                         -- BOR set to 2.85V nominal
       }
pragma fuse_def LPBOR:2  0x40 {          -- Low-Power Brown-out Reset
       DISABLED = 0x40                   -- Low-Power Brown-out Reset disabled
       ENABLED = 0x0                     -- Low-Power Brown-out Reset enabled
       }
--
-- CONFIG2H (0x300003)
--
pragma fuse_def WDT:3  0x3 {             -- Watchdog Timer Enable bits
       ENABLED = 0x3                     -- WDT enabled in hardware (SWDTEN ignored)
       CONTROL = 0x2                     -- WDT controlled by firmware (SWDTEN enabled)
       RUNONLY = 0x1                     -- WDT enabled in hardware, disabled in Sleep mode (SWDTEN ignored)
       DISABLED = 0x0                    -- WDT disabled in hardware (SWDTEN ignored)
       }
pragma fuse_def WDTPS:3  0x3C {          -- Watchdog Timer Postscaler
       P32K = 0x3C                       -- 1:32768
       P16K = 0x38                       -- 1:16384
       P8K = 0x34                        -- 1:8192
       P4K = 0x30                        -- 1:4096
       P2K = 0x2C                        -- 1:2048
       P1K = 0x28                        -- 1:1024
       P512  = 0x24                      -- 1:512
       P256  = 0x20                      -- 1:256
       P128  = 0x1C                      -- 1:128
       P64  = 0x18                       -- 1:64
       P32  = 0x14                       -- 1:32
       P16  = 0x10                       -- 1:16
       P8  = 0xC                         -- 1:8
       P4  = 0x8                         -- 1:4
       P2  = 0x4                         -- 1:2
       P1  = 0x0                         -- 1:1
       }
--
-- CONFIG3H (0x300005)
--
pragma fuse_def CCP2MUX:5  0x1 {         -- CCP2 MUX bit
       pin_C1 = 0x1                      -- CCP2 input/output is multiplexed with RC1
       pin_B3 = 0x0                      -- CCP2 input/output is multiplexed with RB3
       }
pragma fuse_def PBADEN:5  0x2 {          -- PORTB A/D Enable bit
       ANALOG = 0x2                      -- PORTB<5:0> pins are configured as analog input channels on Reset
       DIGITAL = 0x0                     -- PORTB<5:0> pins are configured as digital I/O on Reset
       }
pragma fuse_def T3CKMUX:5  0x10 {        -- Timer3 Clock Input MUX bit
       pin_C0 = 0x10                     -- T3CKI function is on RC0
       pin_B5 = 0x0                      -- T3CKI function is on RB5
       }
pragma fuse_def SDOMUX:5  0x40 {         -- SDO Output MUX bit
       pin_B3 = 0x40                     -- SDO function is on RB3
       pin_C7 = 0x0                      -- SDO function is on RC7
       }
pragma fuse_def MCLR:5  0x80 {           -- Master Clear Reset Pin Enable
       EXTERNAL = 0x80                   -- MCLR pin enabled; RE3 input disabled
       INTERNAL = 0x0                    -- RE3 input pin enabled; external MCLR disabled
       }
--
-- CONFIG4L (0x300006)
--
pragma fuse_def STVR:6  0x1 {            -- Stack Full/Underflow Reset
       ENABLED = 0x1                     -- Stack full/underflow will cause Reset
       DISABLED = 0x0                    -- Stack full/underflow will not cause Reset
       }
pragma fuse_def LVP:6  0x4 {             -- Single-Supply ICSP Enable bit
       ENABLED = 0x4                     -- Single-Supply ICSP enabled if MCLRE is also 1
       DISABLED = 0x0                    -- Single-Supply ICSP disabled
       }
pragma fuse_def ICPRT:6  0x20 {          -- Dedicated In-Circuit Debug/Programming Port Enable
       DISABLED = 0x0                    -- ICPORT disabled
       ENABLED = 0x20                    -- ICPORT enabled
       }
pragma fuse_def XINST:6  0x40 {          -- Extended Instruction Set Enable bit
--     ENABLED = 0x40                    -- NOTE: not supported by JALV2
       DISABLED = 0x0                    -- Instruction set extension and Indexed Addressing mode disabled
       }
pragma fuse_def DEBUG:6  0x80 {          -- Background Debugger Enable bit
       DISABLED = 0x80                   -- Background debugger disabled, RB6 and RB7 configured as general purpose I/O pins
       ENABLED = 0x0                     -- Background debugger enabled, RB6 and RB7 are dedicated to In-Circuit Debug
       }
--
-- CONFIG5L (0x300008)
--
pragma fuse_def CP0:8  0x1 {             -- Block 0 Code Protect
       DISABLED = 0x1                    -- Block 0 is not code-protected
       ENABLED = 0x0                     -- Block 0 is code-protected
       }
pragma fuse_def CP1:8  0x2 {             -- Block 1 Code Protect
       DISABLED = 0x2                    -- Block 1 is not code-protected
       ENABLED = 0x0                     -- Block 1 is code-protected
       }
--
-- CONFIG5H (0x300009)
--
pragma fuse_def CPB:9  0x40 {            -- Boot Block Code Protect
       DISABLED = 0x40                   -- Boot block is not code-protected
       ENABLED = 0x0                     -- Boot block is code-protected
       }
pragma fuse_def CPD:9  0x80 {            -- Data EEPROM Code Protect
       DISABLED = 0x80                   -- Data EEPROM is not code-protected
       ENABLED = 0x0                     -- Data EEPROM is code-protected
       }
--
-- CONFIG6L (0x30000A)
--
pragma fuse_def WRT0:10  0x1 {           -- Block 0 Write Protect
       DISABLED = 0x1                    -- Block 0 (0800-1FFFh) is not write-protected
       ENABLED = 0x0                     -- Block 0 (0800-1FFFh) is write-protected
       }
pragma fuse_def WRT1:10  0x2 {           -- Block 1 Write Protect
       DISABLED = 0x2                    -- Block 1 (2000-3FFFh) is not write-protected
       ENABLED = 0x0                     -- Block 1 (2000-3FFFh) is write-protected
       }
--
-- CONFIG6H (0x30000B)
--
pragma fuse_def WRTC:11  0x20 {          -- Configuration Registers Write Protect
       DISABLED = 0x20                   -- Configuration registers (300000-3000FFh) are not write-protected
       ENABLED = 0x0                     -- Configuration registers (300000-3000FFh) are write-protected
       }
pragma fuse_def WRTB:11  0x40 {          -- Boot Block Write Protect
       DISABLED = 0x40                   -- Boot block (0000-7FFh) is not write-protected
       ENABLED = 0x0                     -- Boot block (0000-7FFh) is write-protected
       }
pragma fuse_def WRTD:11  0x80 {          -- Data EEPROM Write Protect
       DISABLED = 0x80                   -- Data EEPROM is not write-protected
