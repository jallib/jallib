--
const PPS_GROUP        = PPS_0        -- no Peripheral Pin Selection
--
-- Free shared memory: 0x1-0x5F
--
-- ===================================================
--
--    Symbolic Fuse Definitions
-- ------------------------------
--
-- CONFIG1L (0x300000)
--
pragma fuse_def RETEN:0 0x1 {              -- VREG Sleep Enable bit
       ENABLED = 0x1                       -- Enabled
       DISABLED = 0x0                      -- Disabled - Controlled by SRETEN bit
       }
pragma fuse_def INTOSCSEL:0 0x4 {          -- LF-INTOSC Low-power Enable bit
       HP = 0x4                            -- LF-INTOSC in High-power mode during Sleep
       LP = 0x0                            -- LF-INTOSC in Low-power mode during Sleep
       }
pragma fuse_def SOSCSEL:0 0x18 {           -- SOSC Power Selection and mode Configuration bits
       HP = 0x18                           -- High Power SOSC circuit selected
       LP = 0x8                            -- Low Power SOSC circuit selected
       DIG = 0x10                          -- Digital (SCLKI) mode
       }
pragma fuse_def XINST:0 0x40 {             -- Extended Instruction Set
       ENABLED = 0x40                      -- Enabled
       DISABLED = 0x0                      -- Disabled
       }
--
-- CONFIG1H (0x300001)
--
pragma fuse_def OSC:1 0xF {                -- Oscillator
       RC_NOCLKOUT = 0x7                   -- External RC oscillator
       RC_CLKOUT = 0x6                     -- External RC oscillator, CLKOUT function on OSC2
       ECL_NOCLKOUT = 0xD                  -- EC oscillator (Low power, DC - 160 kHz)
       ECL_CLKOUT = 0xC                    -- EC oscillator, CLKOUT function on OSC2 (Low power, DC - 160 kHz)
       ECM_NOCLKOUT = 0xB                  -- EC oscillator (Medium power, 160 kHz - 16 MHz)
       ECM_CLKOUT = 0xA                    -- EC oscillator, CLKOUT function on OSC2 (Medium power, 160 kHz - 16 MHz)
       INTOSC_CLKOUT = 0x9                 -- Internal RC oscillator, CLKOUT function on OSC2
       INTOSC_NOCLKOUT = 0x8               -- Internal RC oscillator
       ECH_NOCLKOUT = 0x5                  -- EC oscillator (High power, 16 MHz - 64 MHz)
       ECH_CLKOUT = 0x4                    -- EC oscillator, CLKOUT function on OSC2 (High power, 16 MHz - 64 MHz)
       HS_MEDIUM = 0x3                     -- HS oscillator (Medium power, 4 MHz - 16 MHz)
       HS_HIGH = 0x2                       -- HS oscillator (High power, 16 MHz - 25 MHz)
       XT = 0x1                            -- XT oscillator
       LP = 0x0                            -- LP oscillator
       }
pragma fuse_def PLLEN:1 0x10 {             -- PLL x4 Enable bit
       P4 = 0x10                           -- Enabled
       P1 = 0x0                            -- Disabled
       }
pragma fuse_def FCMEN:1 0x40 {             -- Fail-Safe Clock Monitor
       DISABLED = 0x0                      -- Disabled
       ENABLED = 0x40                      -- Enabled
       }
pragma fuse_def IESO:1 0x80 {              -- Internal External Oscillator Switch Over Mode
       DISABLED = 0x0                      -- Disabled
       ENABLED = 0x80                      -- Enabled
       }
--
-- CONFIG2L (0x300002)
--
pragma fuse_def PWRTE:2 0x1 {              -- Power Up Timer
       DISABLED = 0x1                      -- Disabled
       ENABLED = 0x0                       -- Enabled
       }
pragma fuse_def BROWNOUT:2 0x6 {           -- Brown Out Detect
       ENABLED = 0x6                       -- Enabled in hardware, SBOREN disabled
       RUNONLY = 0x4                       -- Enabled while active, disabled in SLEEP, SBOREN disabled
       CONTROL = 0x2                       -- Controlled with SBOREN bit
       DISABLED = 0x0                      -- Disabled in hardware, SBOREN disabled
       }
pragma fuse_def VOLTAGE:2 0x18 {           -- Brown-out Reset Voltage bits
       V18 = 0x18                          -- 1.8V
       V20 = 0x10                          -- 2.0V
       V27 = 0x8                           -- 2.7V
       V30 = 0x0                           -- 3.0V
       }
pragma fuse_def BORPWR:2 0x60 {            -- BORMV Power level
       ZERO = 0x60                         -- ZPBORMV instead of BORMV is selected
       HP = 0x40                           -- BORMV set to high power level
       MP = 0x20                           -- BORMV set to medium power level
       LP = 0x0                            -- BORMV set to low power level
       }
--
-- CONFIG2H (0x300003)
--
pragma fuse_def WDT:3 0x3 {                -- Watchdog Timer
       HARDWARE = 0x3                      -- WDT enabled in hardware; SWDTEN bit disabled
       DISABLED = 0x2                      -- WDT controlled by SWDTEN bit setting
       RUNONLY = 0x1                       -- WDT enabled only while device is active and disabled in Sleep mode; SWDTEN bit disabled
       }
pragma fuse_def WDTPS:3 0x7C {             -- Watchdog Postscaler
       P1M = 0x7C                          -- 1:1048576
       P512K = 0x4C                        -- 1:524288
       P256K = 0x48                        -- 1:262144
       P128K = 0x44                        -- 1:131072
       P64K = 0x40                         -- 1:65536
       P32K = 0x3C                         -- 1:32768
       P16K = 0x38                         -- 1:16384
       P8K = 0x34                          -- 1:8192
       P4K = 0x30                          -- 1:4096
       P2K = 0x2C                          -- 1:2048
       P1K = 0x28                          -- 1:1024
       P512  = 0x24                        -- 1:512
       P256  = 0x20                        -- 1:256
       P128  = 0x1C                        -- 1:128
       P64  = 0x18                         -- 1:64
       P32  = 0x14                         -- 1:32
       P16  = 0x10                         -- 1:16
       P8  = 0xC                           -- 1:8
       P4  = 0x8                           -- 1:4
       P2  = 0x4                           -- 1:2
       P1  = 0x0                           -- 1:1
       }
--
-- CONFIG3L (0x300004)
--
pragma fuse_def RTCOSC:4 0x1 {             -- RTCC Clock Select
       T1OSC = 0x1                         -- RTCC uses SOSC
       INTRC = 0x0                         -- RTCC uses INTRC
       }
--
-- CONFIG3H (0x300005)
--
pragma fuse_def CCP2MUX:5 0x1 {            -- CCP2 Mux
       pin_C1 = 0x1                        -- RC1
       pin_E7 = 0x0                        -- RE7-Microcontroller Mode/RB3-All other modes
       }
pragma fuse_def MSSPMASK:5 0x8 {           -- MSSP address masking
       B7 = 0x8                            -- 7 Bit address masking mode
       B5 = 0x0                            -- 5 bit address masking mode
       }
pragma fuse_def MCLR:5 0x80 {              -- Master Clear Enable
       EXTERNAL = 0x80                     -- MCLR Enabled, RG5 Disabled
       INTERNAL = 0x0                      -- MCLR Disabled, RG5 Enabled
       }
--
-- CONFIG4L (0x300006)
--
pragma fuse_def STVR:6 0x1 {               -- Stack Overflow Reset
       ENABLED = 0x1                       -- Enabled
       DISABLED = 0x0                      -- Disabled
       }
pragma fuse_def BBSIZ:6 0x10 {             -- Boot Block Size
       W2K = 0x10                          -- 2K word Boot Block size
       W1K = 0x0                           -- 1K word Boot Block size
       }
pragma fuse_def DEBUG:6 0x80 {             -- Background Debug
       DISABLED = 0x80                     -- Disabled
       ENABLED = 0x0                       -- Enabled
       }
