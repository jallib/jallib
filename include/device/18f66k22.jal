-- ----- address 0xf67 assumed to be SSP2CON1 -----
procedure SSP2MSK'put(byte in x) is
   var byte _ssp2con1_saved = SSP2CON1
   SSP2CON1 = SSP2CON1 &  (!0xf)
   SSP2CON1 = SSP2CON1 | 0x9
   _SSP2MSK = x
   SSP2CON1 = _ssp2con1_saved
end procedure
function SSP2MSK'get() return byte is
   var  byte  x
   var  byte  _ssp2con1_saved = SSP2CON1
   SSP2CON1 = SSP2CON1 &  (!0xf)
   SSP2CON1 = SSP2CON1 | 0x9
   x = _SSP2MSK
   SSP2CON1 = _ssp2con1_saved
   return  x
end function
