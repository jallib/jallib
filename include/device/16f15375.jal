--
-- Free shared memory: 0x70-0x7E
--
-- ------------------------------------------------
--    Symbolic Fuse Definitions
-- ------------------------------------------------
--
-- CONFIG1 (0x8007)
--
pragma fuse_def OSC:0  0x7 {             -- External Oscillator mode selection bits
       ECH_NOCLKOUT = 0x7                -- EC above 8MHz; PFM set to high power
       ECM_NOCLKOUT = 0x6                -- EC for 500kHz to 8MHz; PFM set to medium power
       ECL_NOCLKOUT = 0x5                -- EC below 500kHz; PFM set to low power
       OFF = 0x4                         -- Oscillator not enabled
       HS = 0x2                          -- HS (crystal oscillator) above 4MHz; PFM set to high power
       XT = 0x1                          -- XT (crystal oscillator) above 500kHz, below 4MHz; PFM set to medium power
       LP = 0x0                          -- LP (crystal oscillator) optimized for 32.768kHz; PFM set to low power
       }
pragma fuse_def RSTOSC:0  0x70 {         -- Power-up default value for COSC bits
       EXT1X = 0x70                      -- EXTOSC operating per FEXTOSC bits
       HFINT1 = 0x60                     -- HFINTOSC (1MHz)
       LFINT = 0x50                      -- LFINTOSC
       SOSC = 0x40                       -- SOSC
       EXT4X = 0x20                      -- EXTOSC with 4x PLL, with EXTOSC operating per FEXTOSC bits
       HFINTPLL = 0x10                   -- HFINTOSC with 2x PLL, with OSCFRQ = 16 MHz and CDIV = 1:1 (FOSC = 32 MHz)
       HFINT32 = 0x0                     -- HFINTOSC with OSCFRQ= 32 MHz and CDIV = 1:1
       }
pragma fuse_def CLKOUTEN:0  0x100 {      -- Clock Out Enable bit
       ENABLED = 0x0                     -- CLKOUT function is enabled; FOSC/4 clock appears at OSC2
       DISABLED = 0x100                  -- CLKOUT function is disabled; i/o or oscillator function on OSC2
       }
pragma fuse_def CSWEN:0  0x800 {         -- Clock Switch Enable bit
       ENABLED = 0x800                   -- Writing to NOSC and NDIV is allowed
       DISABLED = 0x0                    -- The NOSC and NDIV bits cannot be changed by user software
       }
pragma fuse_def FCMEN:0  0x2000 {        -- Fail-Safe Clock Monitor Enable bit
       ENABLED = 0x2000                  -- FSCM timer enabled
       DISABLED = 0x0                    -- FSCM timer disabled
       }
--
-- CONFIG2 (0x8008)
--
pragma fuse_def MCLR:1  0x1 {            -- Master Clear Enable bit
       EXTERNAL = 0x1                    -- MCLR pin is Master Clear function
       INTERNAL = 0x0                    -- MCLR pin function is port defined function
       }
pragma fuse_def PWRTE:1  0x2 {           -- Power-up Timer Enable bit
       DISABLED = 0x2                    -- PWRT disabled
       ENABLED = 0x0                     -- PWRT enabled
       }
pragma fuse_def LPBOREN:1  0x20 {        -- Low-Power BOR enable bit
       DISABLED = 0x20                   -- ULPBOR disabled
       ENABLED = 0x0                     -- ULPBOR enabled
       }
pragma fuse_def BROWNOUT:1  0xC0 {       -- Brown-out reset enable bits
       ENABLED = 0xC0                    -- Brown-out Reset Enabled, SBOREN bit is ignored
       RUNONLY = 0x80                    -- Brown-out Reset enabled while running, disabled in sleep; SBOREN is ignored
       CONTROL = 0x40                    -- Brown-out reset enabled according to SBOREN bit
       DISABLED = 0x0                    -- Brown-out reset disabled
       }
pragma fuse_def VOLTAGE:1  0x200 {       -- Brown-out Reset Voltage Selection
       V25 = 0x200                       -- Brown-out Reset Voltage (VBOR) set to 1.9V on LF, and 2.45V on F Devices
       V27 = 0x0                         -- Brown-out Reset Voltage (VBOR) is set to 2.7V
       }
pragma fuse_def ZCD:1  0x400 {           -- Zero-cross detect disable
       DISABLED = 0x400                  -- Zero-cross detect circuit is disabled at POR.
       ENABLED = 0x0                     -- Zero-cross detect circuit is always enabled
       }
pragma fuse_def PPS1WAY:1  0x800 {       -- Peripheral Pin Select one-way control
       ENABLED = 0x800                   -- The PPSLOCK bit can be cleared and set only once in software
       DISABLED = 0x0                    -- The PPSLOCK bit can be set and cleared repeatedly by software
       }
pragma fuse_def STVR:1  0x1000 {         -- Stack Overflow/Underflow Reset Enable bit
       ENABLED = 0x1000                  -- Stack Overflow or Underflow will cause a reset
       DISABLED = 0x0                    -- Stack Overflow or Underflow will not cause a reset
       }
--
-- CONFIG3 (0x8009)
--
pragma fuse_def WDTCPS:2  0x1F {         -- WDT Period Select bits
       F32  = 0x0                        -- Divider ratio 1:32
       F64  = 0x1                        -- Divider ratio 1:64
       F128  = 0x2                       -- Divider ratio 1:128
       F256  = 0x3                       -- Divider ratio 1:256
       F512  = 0x4                       -- Divider ratio 1:512
       F1K = 0x5                         -- Divider ratio 1:1024
       F2K = 0x6                         -- Divider ratio 1:2048
       F4K = 0x7                         -- Divider ratio 1:4096
       F8K = 0x8                         -- Divider ratio 1:8192
       F16K = 0x9                        -- Divider ratio 1:16384
       F32K = 0xA                        -- Divider ratio 1:32768
       F64K = 0xB                        -- Divider ratio 1:65536
       F128K = 0xC                       -- Divider ratio 1:131072
       F256K = 0xD                       -- Divider ratio 1:262144
       F512K = 0xE                       -- Divider ratio 1:524299
       F1M = 0xF                         -- Divider ratio 1:1048576
       F2M = 0x10                        -- Divider ratio 1:2097152
       F4M = 0x11                        -- Divider ratio 1:4194304
       F8M = 0x12                        -- Divider ratio 1:8388608
       SOFTWARE = 0x1F                   -- Divider ratio 1:65536; software control of WDTPS
       }
pragma fuse_def WDT:2  0x60 {            -- WDT operating mode
       DISABLED = 0x0                    -- WDT Disabled, SWDTEN is ignored
       CONTROL = 0x20                    -- WDT enabled/disabled by SWDTEN bit in WDTCON0
       RUNONLY = 0x40                    -- WDT enabled while sleep=0, suspended when sleep=1; SWDTEN ignored
       ENABLED = 0x60                    -- WDT enabled regardless of sleep; SWDTEN ignored
       }
pragma fuse_def WDTCWS:2  0x700 {        -- WDT Window Select bits
       P88 = 0x0                         -- window delay = 87.5 percent of time; no software control; keyed access required
       P75 = 0x100                       -- window delay = 75 percent of time; no software control; keyed access required
       P62 = 0x200                       -- window delay = 62.5 percent of time; no software control; keyed access required
       P50 = 0x300                       -- window delay = 50 percent of time; no software control; keyed access required
       P38 = 0x400                       -- window delay = 37.5 percent of time; no software control; keyed access required
       P25 = 0x500                       -- window delay = 25 percent of time; no software control; keyed access required
       P0 = 0x600                        -- window always open (100%); no software control; keyed access required
       SOFTWARE = 0x700                  -- window always open (100%); software control; keyed access not required
       }
pragma fuse_def WDTCCS:2  0x3800 {       -- WDT input clock selector
       LFINTOSC = 0x0                    -- WDT reference clock is the 31.0kHz LFINTOSC output
       HFINTOSC = 0x800                  -- WDT reference clock is the 31.25 kHz HFINTOSC
       SOSC = 0x1000                     -- WDT reference clock is the 32kHz secondary oscillator
       SOFTWARE = 0x3800                 -- Software Control
       }
--
-- CONFIG4 (0x800A)
--
pragma fuse_def BBSIZ:3  0x7 {           -- Boot Block Size Selection bits
       W512 = 0x7                        -- 512 words boot block size
       W1K = 0x6                         -- 1024 words boot block size
       W2K = 0x5                         -- 2048 words boot block size
       W4K = 0x4                         -- 4096 words boot block size
       W8K = 0x3                         -- * half of user program memory
       W16K = 0x2                        -- * half of user program memory
       W32K = 0x1                        -- * half of user program memory
       W64K = 0x0                        -- * half of user program memory
       }
pragma fuse_def BBEN:3  0x8 {            -- Boot Block Enable bit
       DISABLED = 0x8                    -- Boot Block disabled
       ENABLED = 0x0                     -- Boot Block enabled
       }
pragma fuse_def SAFEN:3  0x10 {          -- SAF Enable bit
       DISABLED = 0x10                   -- SAF disabled
       ENABLED = 0x0                     -- SAF enabled
       }
pragma fuse_def WRTAPP:3  0x80 {         -- Application Block Write Protection bit
       DISABLED = 0x80                   -- Application Block not write protected
       ENABLED = 0x0                     -- Application Block write protected
       }
pragma fuse_def WRTB:3  0x100 {          -- Boot Block Write Protection bit
       DISABLED = 0x100                  -- Boot Block not write protected
       ENABLED = 0x0                     -- Boot Block write protected
       }
pragma fuse_def WRTC:3  0x200 {          -- Configuration Register Write Protection bit
       DISABLED = 0x200                  -- Configuration Register not write protected
       ENABLED = 0x0                     -- Configuration Register write protected
       }
pragma fuse_def WRTSAF:3  0x800 {        -- Storage Area Flash Write Protection bit
       DISABLED = 0x800                  -- SAF not write protected
       ENABLED = 0x0                     -- SAF write protected
       }
pragma fuse_def LVP:3  0x2000 {          -- Low Voltage Programming Enable bit
       ENABLED = 0x2000                  -- Low Voltage programming enabled. MCLR/Vpp pin function is MCLR.
       DISABLED = 0x0                    -- High Voltage on MCLR/Vpp must be used for programming
       }
--
-- CONFIG5 (0x800B)
--
pragma fuse_def CP:4  0x1 {              -- UserNVM Program memory code protection bit
       DISABLED = 0x1                    -- UserNVM code protection disabled
       ENABLED = 0x0                     -- UserNVM code protection enabled
       }
