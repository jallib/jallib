const PPS_GROUP        = PPS_0        -- no Peripheral Pin Selection
--
-- Free shared memory: 0x1-0x5F
--
-- ===================================================
--
--    Symbolic Fuse Definitions
-- ------------------------------
--
-- CONFIG1H (0x300001)
--
pragma fuse_def OSC:1 0x7 {                -- oscillator selection bits
       RC_NOCLKOUT = 0x7                   -- rc oscillator w/ osc2 configured as ra6
       HS_PLL = 0x6                        -- hs oscillator with pll enabled; clock frequency = (4 x fosc)
       EC_NOCLKOUT = 0x5                   -- ec oscillator w/ osc2 configured as ra6
       EC_CLKOUT = 0x4                     -- ec oscillator w/ osc2 configured as divide-by-4 clock output
       RC_CLKOUT = 0x3                     -- rc oscillator w/ osc2 configured as divide-by-4 clock output
       HS = 0x2                            -- hs oscillator
       XT = 0x1                            -- xt oscillator
       LP = 0x0                            -- lp oscillator
       }
pragma fuse_def OSCS:1 0x20 {              -- oscillator system clock switch enable bit
       DISABLED = 0x20                     -- oscillator system clock switch option is disabled (main oscillator is source)
       ENABLED = 0x0                       -- timer1 oscillator system clock switch option is enabled (oscillator switching is enabled)
       }
--
-- CONFIG2L (0x300002)
--
pragma fuse_def PWRTE:2 0x1 {              -- power-up timer enable bit
       DISABLED = 0x1                      -- pwrt disabled
       ENABLED = 0x0                       -- pwrt enabled
       }
pragma fuse_def BROWNOUT:2 0x2 {           -- brown-out reset enable bit
       ENABLED = 0x2                       -- brown-out reset enabled
       DISABLED = 0x0                      -- brown-out reset disabled
       }
pragma fuse_def VOLTAGE:2 0xC {            -- brown-out reset voltage bits
       V25 = 0xC                           -- vbor set to 2.5v
       V27 = 0x8                           -- vbor set to 2.7v
       V42 = 0x4                           -- vbor set to 4.2v
       V45 = 0x0                           -- vbor set to 4.5v
       }
--
-- CONFIG2H (0x300003)
--
pragma fuse_def WDT:3 0x1 {                -- watchdog timer enable bit
       ENABLED = 0x1                       -- wdt enabled
       DISABLED = 0x0                      -- wdt disabled (control is placed on the swdten bit)
       }
pragma fuse_def WDTPS:3 0xE {              -- watchdog timer postscale select bits
       P128  = 0xE                         -- 1:128
       P64  = 0xC                          -- 1:64
       P32  = 0xA                          -- 1:32
       P16  = 0x8                          -- 1:16
       P8  = 0x6                           -- 1:8
       P4  = 0x4                           -- 1:4
       P2  = 0x2                           -- 1:2
       P1  = 0x0                           -- 1:1
       }
--
-- CONFIG3L (0x300004)
--
pragma fuse_def PMODE:4 0x3 {              -- processor mode select bits
       MICROCONTROLLER = 0x3               -- microcontroller mode
       MICROPROCESSOR = 0x2                -- microprocessor mode
       MICROPROCESSOR_BOOT = 0x1           -- microprocessor with boot block mode
       EXT = 0x0                           -- extended microcontroller mode
       }
pragma fuse_def WAIT:4 0x80 {              -- external bus data wait enable bit
       DISABLED = 0x80                     -- wait selections unavailable for table reads and table writes
       ENABLED = 0x0                       -- wait selections for table reads and table writes are determined by the wait1:wait0 bits (memcom<5:4>)
       }
--
-- CONFIG3H (0x300005)
--
pragma fuse_def CCP2MUX:5 0x1 {            -- ccp2 mux bit
       ENABLED = 0x1                       -- ccp2 input/output is multiplexed with rc1
       pin_E7 = 0x0                        -- ccp2 is multiplexed with rb3 in extended microcontroller, microprocessor or microprocessor with boot block mode. or with re7 in microcontroller mode.
       }
--
-- CONFIG4L (0x300006)
--
pragma fuse_def STVR:6 0x1 {               -- stack full/underflow reset enable bit
       ENABLED = 0x1                       -- stack full/underflow will cause reset
       DISABLED = 0x0                      -- stack full/underflow will not cause reset
       }
pragma fuse_def LVP:6 0x4 {                -- low-voltage icsp enable bit
       ENABLED = 0x4                       -- low-voltage icsp enabled
       DISABLED = 0x0                      -- low-voltage icsp disabled
       }
pragma fuse_def DEBUG:6 0x80 {             -- background debugger enable bit
       DISABLED = 0x80                     -- background debugger disabled. rb6 and rb7 configured as general purpose i/o pins.
       ENABLED = 0x0                       -- background debugger enabled. rb6 and rb7 are dedicated to in-circuit debug.
       }
--
-- CONFIG5L (0x300008)
--
pragma fuse_def CP0:8 0x1 {                -- code protection bit
