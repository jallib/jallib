pragma fuse_def WDTPS:3  0xF {           -- Watchdog Postscaler
       P32K = 0xF                        -- 1:32768
       P16K = 0xE                        -- 1:16384
       P8K = 0xD                         -- 1:8192
       P4K = 0xC                         -- 1:4096
       P2K = 0xB                         -- 1:2048
       P1K = 0xA                         -- 1:1024
