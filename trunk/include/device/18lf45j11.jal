       P256  = 0x8                       -- 1:256
       P128  = 0x7                       -- 1:128
       P64  = 0x6                        -- 1:64
       P32  = 0x5                        -- 1:32
       P16  = 0x4                        -- 1:16
       P8  = 0x3                         -- 1:8
       P4  = 0x2                         -- 1:4
       P2  = 0x1                         -- 1:2
       P1  = 0x0                         -- 1:1
       }
--
