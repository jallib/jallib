-- ===================================================
--
--    Miscellaneous
--    -------------
--
const PPS_GROUP        = PPS_0        -- no Peripheral Pin Selection
--
-- Free shared memory: 0x1-0x5F
--
-- ===================================================
--
--    Symbolic Fuse Definitions
-- ------------------------------
--
-- CONFIG1H (0x300001)
--
pragma fuse_def OSC:1 0xF {                -- Oscillator
       RC_CLKOUT = 0xC                     -- 11XX EXT RC-CLKOUT on RA6
       INTOSC_CLKOUT = 0x9                 -- Internal oscillator block, CLKO function on RA6, port function on RA7
       INTOSC_NOCLKOUT = 0x8               -- Internal oscillator block, port function on RA6 and RA7
       RC_NOCLKOUT = 0x7                   -- External RC oscillator, port function on RA6
       HS_PLL = 0x6                        -- HS oscillator, PLL enabled (clock frequency = 4 x FOSC1)
       EC_NOCLKOUT = 0x5                   -- EC oscillator, port function on RA6
       EC_CLKOUT = 0x4                     -- EC oscillator, CLKO function on RA6
       HS = 0x2                            -- HS oscillator
       XT = 0x1                            -- XT oscillator
       LP = 0x0                            -- LP oscillator
       }
pragma fuse_def FCMEN:1 0x40 {             -- Fail-Safe Clock Monitor Enable
       DISABLED = 0x0                      -- Fail-Safe Clock Monitor disabled
       ENABLED = 0x40                      -- Fail-Safe Clock Monitor enabled
       }
pragma fuse_def IESO:1 0x80 {              -- Internal External Switch Over Mode
       DISABLED = 0x0                      -- Oscillator Switchover mode disabled
       ENABLED = 0x80                      -- Oscillator Switchover mode enabled
       }
--
-- CONFIG2L (0x300002)
--
pragma fuse_def PWRTE:2 0x1 {              -- Power Up Timer
       DISABLED = 0x1                      -- PWRT disabled
       ENABLED = 0x0                       -- PWRT enabled
       }
pragma fuse_def BROWNOUT:2 0x6 {           -- Brown Out Detect
       ENABLED = 0x6                       -- Brown-out Reset enabled in hardware only (SBOREN is disabled)
       RUNONLY = 0x4                       -- Brown-out Reset enabled in hardware only and disabled in Sleep mode (SBOREN is disabled)
       CONTROL = 0x2                       -- Brown-out Reset enabled and controlled by software (SBOREN is enabled)
       DISABLED = 0x0                      -- Brown-out Reset disabled in hardware and software
       }
pragma fuse_def VOLTAGE:2 0x18 {           -- Brown Out Voltage
       V20 = 0x18                          -- VBOR set to 2.1V
       V28 = 0x10                          -- VBOR set to 2.8V
       V43 = 0x8                           -- VBOR set to 4.3V
       V46 = 0x0                           -- VBOR set to 4.6V
       }
--
-- CONFIG2H (0x300003)
--
pragma fuse_def WDT:3 0x1 {                -- Watchdog Timer
       ENABLED = 0x1                       -- WDT enabled
       DISABLED = 0x0                      -- WDT disabled (control is placed on the SWDTEN bit)
       }
pragma fuse_def WDTPS:3 0x1E {             -- Watchdog Postscaler
       P32K = 0x1E                         -- 1:32768
       P16K = 0x1C                         -- 1:16384
       P8K = 0x1A                          -- 1:8192
       P4K = 0x18                          -- 1:4096
       P2K = 0x16                          -- 1:2048
       P1K = 0x14                          -- 1:1024
       P512  = 0x12                        -- 1:512
       P256  = 0x10                        -- 1:256
       P128  = 0xE                         -- 1:128
       P64  = 0xC                          -- 1:64
       P32  = 0xA                          -- 1:32
       P16  = 0x8                          -- 1:16
       P8  = 0x6                           -- 1:8
       P4  = 0x4                           -- 1:4
       P2  = 0x2                           -- 1:2
       P1  = 0x0                           -- 1:1
       }
--
-- CONFIG3L (0x300004)
--
pragma fuse_def PMODE:4 0x3 {              -- Processor Mode
       MICROCONTROLLER = 0x3               -- Microcontroller mode
       MICROPROCESSOR = 0x2                -- Microprocessor mode
       MICROPROCESSOR_BOOT = 0x1           -- Microprocessor with Boot Block mode
       EXT = 0x0                           -- Extended Microcontroller mode
       }
pragma fuse_def BW:4 0x40 {                -- Bus Width
       B16 = 0x40                          -- 16-bit External Bus Data Width
       B8 = 0x0                            -- 8-bit External Bus Data Width
       }
pragma fuse_def WAIT:4 0x80 {              -- External Bus Wait
       DISABLED = 0x80                     -- Wait selections unavailable, device will not wait
       ENABLED = 0x0                       -- Wait programmed by WAIT1 and WAIT0 bits of MEMCOM register (MEMCOM<5:4>)
       }
--
-- CONFIG3H (0x300005)
--
pragma fuse_def CCP2MUX:5 0x1 {            -- CCP2 Mux
       pin_C1 = 0x1                        -- CCP2 input/output is multiplexed with RC1
       pin_E7 = 0x0                        -- CCP2 is multiplexed with RB3 in Extended Microcontroller, Microprocessor or Microprocessor with Boot Block mode. Or with RE7 in Microcontroller mode.
       }
pragma fuse_def LPT1OSC:5 0x4 {            -- Low Power Timer1 Osc enable
       LOW_POWER = 0x4                     -- Timer1 configured for low-power operation
       HIGH_POWER = 0x0                    -- Timer1 configured for higher power operation
       }
pragma fuse_def MCLR:5 0x80 {              -- Master Clear Enable
       EXTERNAL = 0x80                     -- MCLR pin enabled; RG5 input pin disabled
       INTERNAL = 0x0                      -- RG5 input pin enabled; MCLR disabled
       }
--
-- CONFIG4L (0x300006)
--
pragma fuse_def STVR:6 0x1 {               -- Stack Overflow Reset
       ENABLED = 0x1                       -- Stack full/underflow will cause Reset
       DISABLED = 0x0                      -- Stack full/underflow will not cause Reset
       }
pragma fuse_def XINST:6 0x40 {             -- Extended Instruction Set Enable bit
       ENABLED = 0x40                      -- Instruction set extension and Indexed Addressing mode enabled
       DISABLED = 0x0                      -- Instruction set extension and Indexed Addressing mode disabled (Legacy mode)
       }
pragma fuse_def DEBUG:6 0x80 {             -- Background Debugger Enable bit
       DISABLED = 0x80                     -- Background debugger disabled, RB6 and RB7 configured as general purpose I/O pins
       ENABLED = 0x0                       -- Background debugger enabled, RB6 and RB7 are dedicated to In-Circuit Debug
       }
--
-- CONFIG5L (0x300008)
--
pragma fuse_def CP:8 0x1 {                 -- Code Protect 00000-03FFF
       DISABLED = 0x1                      -- Program memory block not code-protected
       ENABLED = 0x0                       -- Program memory block code-protected
       }
--
-- CONFIG7L (0x30000C)
--
pragma fuse_def EBTR:12 0x1 {              -- Table Read Protect 00000-03FFF
       DISABLED = 0x1                      -- Internal program memory block not protected from table reads executed from external memory block
       ENABLED = 0x0                       -- Internal program memory block protected from table reads executed from external memory block
       }
--
